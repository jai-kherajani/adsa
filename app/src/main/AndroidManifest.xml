<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adsa">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ADSAApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ADSA"
        android:usesCleartextTraffic="true">
        <activity android:name=".Covid19Activity"/>
        <activity android:name=".WellBeingActivity" />
        <activity android:name=".PoliciesActivity"/>
        <activity android:name=".AboutActivity" />
        <activity android:name=".OtherActivity" />
        <activity android:name=".IssueDetails" />
        <activity android:name=".JiraActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".DirectoryActivity" />
        <activity android:name=".IdCardActivity" />
        <activity android:name=".ComingSoon" />
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>